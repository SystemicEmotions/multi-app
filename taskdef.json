{
    "executionRoleArn": "arn:aws:iam::450507733673:role/ecsTaskRole,
    "TaskRoleArn": "arn:aws:iam::450507733673:role/ecsTaskRole",
    "containerDefinitions": [
        {
            "name": "sample-website",
            "image": "alokmalakar/demoapp",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "multi-app"
}
